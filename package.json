{
  "name": "doorways-web",
  "version": "0.0.24",
  "private": true,
  "scripts": {
    "start": "node .devOps/webpack/scripts/start.js",
    "build": "sh -ac '. ./.env.production; node .devOps/webpack/scripts/build.js'",
    "upgrade": "yarn upgrade-interactive --latest",
    "deploy:before": "yarn run build && aws s3 rm s3://app.joindoorways.com --recursive --profile doorways",
    "deploy:after": "aws cloudfront create-invalidation --distribution-id EL5IQBM2NGJKS --paths \"/*\" --profile doorways && rm -R ./build",
    "deploy": "yarn run deploy:before && aws s3 cp ./build s3://app.joindoorways.com --recursive --exclude .DS_Store --profile doorways && yarn run deploy:after"
  },
